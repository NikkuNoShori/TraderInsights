# WebSocket token for development
VITE_WS_TOKEN=development

# API URL
VITE_API_URL=http://localhost:3000

# Polygon.io API Configuration
VITE_POLYGON_API_KEY=your_polygon_api_key_here

# Environment
NODE_ENV=development

