VITE_API_KEY=your_api_key

# Feature Flags
VITE_ENABLE_REAL_TIME_TRADING=false
VITE_ENABLE_PAPER_TRADING=false
VITE_ENABLE_ANALYTICS=true

# Trading API Configuration
VITE_TRADING_API_URL=https://api.trading.example.com
VITE_TRADING_API_KEY=your_trading_api_key
VITE_TRADING_WEBSOCKET_URL=wss://ws.trading.example.com

# Market Data Configuration
VITE_MARKET_DATA_API_URL=https://api.marketdata.example.com
VITE_MARKET_DATA_API_KEY=your_market_data_api_key
VITE_MARKET_DATA_REFRESH_INTERVAL=5000

# Application Configuration
VITE_APP_NAME=TraderInsights
VITE_APP_VERSION=1.0.0
VITE_APP_ENV=development
